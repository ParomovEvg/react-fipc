{
  "version": "0.4.0",
  "description": "simple implementation of dependency injection in react",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ParomovEvg/react-fipc.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "di",
    "dependency-injection",
    "currying"
  ],
  "bugs": {
    "url": "https://github.com/ParomovEvg/react-fipc/issues"
  },
  "homepage": "https://github.com/ParomovEvg/react-fipc#readme",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-fipc",
  "author": "Evgeny Paromov",
  "module": "dist/react-fipc.esm.js",
  "size-limit": [
    {
      "path": "dist/react-fipc.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-fipc.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2"
  }
}
